import { Component } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'layout-full-height',  templateUrl: './layout-full-height.html'})export class LayoutFullHeightPage {  constructor(private appSettings: AppSettings) { }    ngOnInit() {    this.appSettings.appContentClass = 'p-0';    this.appSettings.appContentFullHeight = true;  }  ngOnDestroy() {    this.appSettings.appContentClass = '';    this.appSettings.appContentFullHeight = false;  }}