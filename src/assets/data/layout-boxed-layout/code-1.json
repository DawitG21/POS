import { Component } from '@angular/core';import { AppSettings } from '../../../service/app-settings.service';@Component({  selector: 'layout-boxed-layout',  templateUrl: './layout-boxed-layout.html'})export class LayoutBoxedLayoutPage {  constructor(private appSettings: AppSettings) { }    ngOnInit() {    this.appSettings.appBoxedLayout = true;  }  ngOnDestroy() {    this.appSettings.appBoxedLayout = false;  }}